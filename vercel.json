{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/submit",
            "dest": "/api/submit.js"
        },
        {
            "src": "/api/submit-reactions",
            "dest": "/api/submit-reactions.js"
        },
        {
            "src": "/reactions",
            "dest": "/reactions.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}