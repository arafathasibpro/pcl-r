{
    "rewrites": [
        {
            "source": "/api/submit",
            "destination": "/api/submit"
        },
        {
            "source": "/api/submit-reactions",
            "destination": "/api/submit-reactions"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "POST, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type"
                }
            ]
        }
    ]
}